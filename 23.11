#include <iostream>
#include <cstdlib>  
#include <ctime>   

using namespace std;

void createM(int firstM[3][3], int secondM[5][5]) {
    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            secondM[i + 1][j + 1] = firstM[i][j];
        }
    }

    for (int i = 0; i < 5; ++i) {
        secondM[i][0] = rand() % 100;
        secondM[i][4] = rand() % 100;
    }

    for (int j = 0; j < 5; ++j) {
        secondM[0][j] = rand() % 100;
        secondM[4][j] = rand() % 100;
    }
}


int main() {
    int mas[3][3];

    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            mas[i][j] = rand() % 100;
        }
    }

    cout << "Исходная матрица:" << endl;
    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            cout << mas[i][j] << " ";
        }
        cout << endl;
    }

    int secondM[5][5];
    createM(mas, secondM);

    cout << "Расширенная матрица:" << endl;
    for (int i = 0; i < 5; ++i) {
        for (int j = 0; j < 5; ++j) {
            cout << secondM[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}

 
 
